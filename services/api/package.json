{
  "name": "@omx-web/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start:dev": "node --watch --watch-path src -r ts-node/register src/main.ts",
    "start:prod": "node dist/main.js",
    "test": "TS_NODE_PROJECT=tsconfig.json TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"moduleResolution\":\"node\"}' node -r ts-node/register --test --experimental-test-coverage --test-reporter=spec tests/*.test.ts"
  },
  "dependencies": {
    "@fastify/static": "^8.1.1",
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/swagger": "^11.2.1",
    "bullmq": "^5.53.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^5.2.1",
    "ioredis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
